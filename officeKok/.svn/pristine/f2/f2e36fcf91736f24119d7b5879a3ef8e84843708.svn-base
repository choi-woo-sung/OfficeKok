<?xml version="1.0" encoding="UTF-8" ?>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>

<meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport" />
<meta content="" name="description" />
<meta content="" name="author" />

<!-- ================== BEGIN BASE CSS STYLE ================== -->
<link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900" rel="stylesheet" />
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
<link href="${pageContext.request.contextPath}/template/assets/css/google/app.min.css" rel="stylesheet" />
<!-- ================== END BASE CSS STYLE ================== -->

<title>OfficeKok</title>

<!-- begin login-cover -->
<div class="login-cover">
	<div class="login-cover-image" style="background-image: 
		url(${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-17.jpg)"
		data-id="login-cover-image"></div>
	<div class="login-cover-bg"></div>
</div>
<!-- end login-cover -->

<!-- begin login -->
<div class="login login-v2" data-pageload-addclass="animated fadeIn">
	<!-- begin brand -->
	<div class="login-header"> <!-- OfficeKok 글자, 자물쇠 아이콘 -->
		<div class="brand"><span class="logo"></span><b class="text-blue">OfficeKok</b></div>
		<div class="icon"><i class="fa fa-lock" ></i></div>
	</div>
	<!-- end brand -->
	
	<!-- begin login-content -->
	<div class="login-content">
		<form action="${pageContext.request.contextPath }/login" method="POST" class="margin-bottom-0">
			<!-- 이메일 입력 -->
			<div class="form-group m-b-20">
				<input type="email" name="empmail" class="form-control form-control-lg" placeholder="Email Address" required />
			</div>
			
			<!-- 비밀번호 입력 -->
			<div class="form-group m-b-20">
				<input type="password" name="emppw" class="form-control form-control-lg" placeholder="Password" required />
			</div>
			
			<!-- 저장하기 -->
			<div class="checkbox checkbox-css m-b-20">
				<input type="checkbox" id="remember_checkbox" name="saveId" value="save" />
				<label for="remember_checkbox" > 저장하기 </label>
			</div>
			
			<!-- 로그인 버튼 -->
			<div class="login-buttons">
				<button type="submit" class="btn btn-primary btn-block btn-lg">로그인</button>
			</div>
			
			<!-- 아이디 비밀번호 찾기 -->
			<div style="width: 100%; text-align: center;" class="m-t-20">
				<a href="javascript:;">아이디 찾기</a>
				 &nbsp;&nbsp;|&nbsp;&nbsp;
				<a href="javascript:;">비밀번호 찾기</a>
			</div>
		</form>
	</div>
	<!-- end login-content -->
</div>
<!-- end login -->

<!-- begin login-bg -->
<ul class="login-bg-list clearfix">
	<li class="active"><a href="javascript:;" data-click="change-bg"
		data-img="${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-17.jpg"
		style="background-image: url(${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-17.jpg)"></a></li>
	<li><a href="javascript:;" data-click="change-bg"
		data-img="${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-16.jpg"
		style="background-image: url(${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-16.jpg)"></a></li>
	<li><a href="javascript:;" data-click="change-bg"
		data-img="${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-15.jpg"
		style="background-image: url(${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-15.jpg)"></a></li>
	<li><a href="javascript:;" data-click="change-bg"
		data-img="${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-14.jpg"
		style="background-image: url(${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-14.jpg)"></a></li>
	<li><a href="javascript:;" data-click="change-bg"
		data-img="${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-13.jpg"
		style="background-image: url(${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-13.jpg)"></a></li>
	<li><a href="javascript:;" data-click="change-bg"
		data-img="${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-12.jpg"
		style="background-image: url(${pageContext.request.contextPath}/template/assets/img/login-bg/login-bg-12.jpg)"></a></li>
</ul>
<!-- end login-bg -->

<!-- ================== BEGIN BASE JS ================== -->
<script src="${pageContext.request.contextPath}/template/assets/js/app.min.js"></script>
<script src="${pageContext.request.contextPath}/template/assets/js/theme/google.min.js"></script>
